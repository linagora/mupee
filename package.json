  "repository": {
    "type": "git",
    "url": "https://github.com/linagora/mupee.git"
  },
  "dependencies": {
    "express": "3.4.x",
    "request": "2.27.x",
    "jade": "*",
    "mongoskin": "0.6.x",
    "mongodb": "1.3.x",
    "jstoxml": "0.2.x",
    "libxml-to-js": "0.3.x",
    "winston": "0.7.x",
    "nconf": "0.6.x",
    "mkdirp": "0.3.x",
    "async": "*",
    "passport": "*",
    "passport-http": "*",
    "async": "~0.2.9",
    "clone": "~0.1.11"
  },
  "devDependencies": {
    "mocha": "*",
    "chai": "*",
    "nock": "*",
    "phantomjs": "*",
    "karma": "*",
    "karma-junit-reporter": "*",
    "karma-jasmine": "*",
    "karma-ng-scenario": "*",
    "karma-phantomjs-launcher": "*",
    "karma-mocha": "*",
    "mockery": "*",
    "grunt": "*",
