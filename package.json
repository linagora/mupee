    "karma": "0.10.4",
    "karma-coverage": "0.1.0",
    "karma-junit-reporter": "0.1.0",
    "karma-spec-reporter": "0.0.6",
    "karma-jasmine": "0.1.3",
    "karma-ng-scenario": "0.1.0",
    "karma-phantomjs-launcher": "0.1.0",
    "karma-mocha": "0.1.0",
    "mockery": "1.4.0",
