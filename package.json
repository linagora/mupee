    "test": "sh ./scripts/test-backend.sh && sh ./scripts/test-frontend.sh",
    "postinstall": "sh ./scripts/browserify.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/linagora/mupee.git"
  },
  "dependencies": {
    "express": "3.4.4",
    "request": "2.27.0",
    "jade": "0.35.0",
    "mongoskin": "0.6.0",
    "mongodb": "1.3.19",
    "jstoxml": "0.2.2",
    "libxml-to-js": "0.3.11",
    "winston": "0.7.2",
    "nconf": "0.6.8",
    "mkdirp": "0.3.5",
    "async": "0.2.9",
    "passport": "0.1.17",
    "passport-http": "0.2.2",
    "clone": "0.1.11",
    "mozilla-version-comparator": "~1.0.2",
    "browserify": "2.35.0",
    "adm-zip": "~0.4.3",
    "fs-extra": "~0.8.1",
    "checksum": "~0.1.1",
    "flat": "~1.0.0",
    "node-uuid": "~1.4.1"
